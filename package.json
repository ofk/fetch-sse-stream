{
  "name": "fetch-sse-stream",
  "version": "0.0.4",
  "description": "A library for Server-Sent Events, like fetch.",
  "license": "MIT",
  "author": "ofk <ofkjpn+npm@gmail.com> (https://github.com/ofk/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ofk/fetch-sse-stream.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint .",
    "lint:package-json": "prettier-package-json -l",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "npm run lint:eslint -- --fix",
    "lint-fix:package-json": "prettier-package-json --write",
    "lint-fix:prettier": "prettier --write .",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "dependencies": {
    "es6-error": "^4.1.1",
    "eventsource-parser": "^3.0.0"
  },
  "devDependencies": {
    "@ofk/eslint-config-recommend": "^7.1.2",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "nock": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-package-json": "^2.8.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "keywords": [],
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "prettier-package-json --write"
    ],
    "*.{js,jsx,mjs,cjs,json,md}": "prettier --write",
    "*.{ts,tsx,mts,cts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "npm@10.8.0+sha512.c21f77b91733829ec70e73cc88b5dc0a4bf685a81d252d3327d293ff7d5dd05a173f4dbeaa037600ec29696f397f14569229e5dab10b7cfc3e0a30575b8f3f8d"
}
